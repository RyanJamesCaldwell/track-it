<% provide(:title, 'New Project') %>
<h1>New Project</h1>

<%= form_with(model: @project, local: true) do |f| %>
  <%= f.label :name %>
  <br />
  <%= f.text_field :name %>
  <br />

  <%= f.label :description %>
  <br />
  <%= f.text_area :description %>
  <br />

  <%= f.submit 'Create Project' %>
<% end %>
